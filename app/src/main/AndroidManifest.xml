<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:targetApi="31">
        <activity
            android:name=".Screens_Activity.setting.User_Manual"
            android:exported="false" />
        <activity
            android:name=".Screens_Activity.TaskDetails.TaskDetails"
            android:exported="false" />
        <activity
            android:name=".Screens_Activity.setting.projects.Detail_Project_View"
            android:exported="false" />
        <activity
            android:name=".Screens_Activity.setting.projects.ProjectList"
            android:exported="false" />
        <activity
            android:name=".Screens_Activity.setting.ProfileScreen.UserDataForm.UserDataForm"
            android:exported="false" />
        <activity
            android:name=".Screens_Activity.setting.ProfileScreen.Profile_Screen"
            android:exported="false" />
        <activity
            android:name=".splash_pages.splash_screen"
            android:exported="false" />
        <activity
            android:name=".Screens_Activity.HomePage"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>